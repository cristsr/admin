    "worker": "angular-http-server --path dist/admin -p 9000",
    "test": "ng test",
    "lint": "ng lint --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "e2e": "ng e2e",
    "prepare": "husky install"
  },
